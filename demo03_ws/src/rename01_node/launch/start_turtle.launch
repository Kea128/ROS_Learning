<!-- 需要启动多个乌龟GUI节点 -->
<launch>
    <!-- 默认的乌龟节点名称就是turtlesim -->
    <node pkg="turtlesim" type="turtlesim_node" name="turtlesim" /> 
    <!-- 名称重映射 -->
    <node pkg="turtlesim" type="turtlesim_node" name="t1" />
    <!-- 命名空间 -->
    <node pkg="turtlesim" type="turtlesim_node" name="turtlesim" ns="home1" />
    <!-- 名称重映射+命名空间 -->
    <node pkg="turtlesim" type="turtlesim_node" name="t2" ns="home2" />
    

</launch>